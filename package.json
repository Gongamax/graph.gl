  },
  "bugs": {
    "url": "https://github.com/Gongamax/graph.gl/issues"
  },
  "homepage": "https://github.com/Gongamax/graph.gl#readme",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
